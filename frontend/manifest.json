{
  "manifest_version": 2,
  "name": "BigBlueButton downloader",
  "version": "0.0.5",
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://vconf.tomtit-tomsk.ru/playback/presentation/*"
      ],
      "js": [
        "index.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}

